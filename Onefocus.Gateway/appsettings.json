{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Reverseproxy": {
    "Routes": {
      "membership": {
        "ClusterId": "membership",
        "AuthorizationPolicy": "anonymous",
        "Match": {
          "Path": "api/membership/{**catch-all}"
        },
        "Transforms": [ { "PathPattern": "{**catch-all}" } ]
      },
      "membership_swagger": {
        "ClusterId": "membership",
        "AuthorizationPolicy": "anonymous",
        "Match": {
          "Path": "api/membership/swagger/{**catch-all}"
        },
        "Transforms": [ { "PathPattern": "swagger/{**catch-all}" } ]
      },
      "identity": {
        "ClusterId": "identity",
        "Match": {
          "Path": "api/identity/{**catch-all}"
        },
        "Transforms": [ { "PathPattern": "{**catch-all}" } ]
      },
      "wallet": {
        "ClusterId": "wallet",
        "Match": {
          "Path": "api/wallet/{**catch-all}"
        },
        "Transforms": [ { "PathPattern": "{**catch-all}" } ]
      },
      "home": {
        "ClusterId": "home",
        "AuthorizationPolicy": "anonymous",
        "Match": {
          "Path": "api/home/{**catch-all}"
        },
        "Transforms": [ { "PathPattern": "{**catch-all}" } ]
      },
      "home_swagger": {
        "ClusterId": "home",
        "AuthorizationPolicy": "anonymous",
        "Match": {
          "Path": "api/home/swagger/{**catch-all}"
        },
        "Transforms": [ { "PathPattern": "swagger/{**catch-all}" } ]
      },
      "web": {
        "ClusterId": "web",
        "AuthorizationPolicy": "anonymous",
        "Match": {
          "Path": "/{**catch-all}"
        },
        "Transforms": [ { "PathPattern": "{**catch-all}" } ]
      }
    },
    "Clusters": {
      "membership": {
        "Destinations": {
          "destination1": {
            "Address": "http://membership.onefocus.com"
          }
        }
      },
      "identity": {
        "Destinations": {
          "destination1": {
            "Address": "http://identity.onefocus.com"
          }
        }
      },
      "wallet": {
        "Destinations": {
          "destination1": {
            "Address": "http://wallet.onefocus.com"
          }
        }
      },
      "home": {
        "Destinations": {
          "destination1": {
            "Address": "http://home.onefocus.com"
          }
        }
      },
      "web": {
        "Destinations": {
          "destination1": {
            "Address": "http://www.onefocus.com"
          }
        }
      }
    }
  }
}
