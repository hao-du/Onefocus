<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net9.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
	</PropertyGroup>

	<ItemGroup>
	  <None Remove="MachineLearning\onnx\config.json" />
	  <None Remove="MachineLearning\onnx\genai_config.json" />
	  <None Remove="MachineLearning\onnx\model.onnx" />
	  <None Remove="MachineLearning\onnx\tokenizer.json" />
	</ItemGroup>

	<ItemGroup>
	  <Content Include="MachineLearning\onnx\config.json">
	    <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
	  </Content>
	  <Content Include="MachineLearning\onnx\genai_config.json">
	    <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
	  </Content>
	  <Content Include="MachineLearning\onnx\model.onnx">
	    <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
	  </Content>
	  <Content Include="MachineLearning\onnx\tokenizer.json">
	    <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
	  </Content>
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="MassTransit.RabbitMQ" Version="8.5.5" />
		<PackageReference Include="Microsoft.ML" Version="5.0.0" />
		<PackageReference Include="Microsoft.ML.OnnxRuntime" Version="1.23.2" />
		<PackageReference Include="Microsoft.ML.Tokenizers" Version="2.0.0" />
		<PackageReference Include="OpenSearch.Client" Version="1.8.0" />
		<PackageReference Include="OpenSearch.Net" Version="1.8.0" />
	</ItemGroup>

	<ItemGroup>
	  <ProjectReference Include="..\..\Onefocus.Common\Onefocus.Common.csproj" />
	  <ProjectReference Include="..\Onefocus.Search.Application\Onefocus.Search.Application.csproj" />
	</ItemGroup>

</Project>
